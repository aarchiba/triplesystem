     "input": [
      "profile = np.zeros(16)\n",
      "profile[:len(profile)//2] = 1\n",
      "F = np.fft.rfft(profile)\n",
      "for shift in np.linspace(0,1,8,endpoint=False):\n",
      "    s = np.fft.irfft(F*np.exp(2.j*np.pi*shift/len(profile)*np.arange(len(F))))\n",
      "    plt.plot(s, drawstyle=\"steps-mid\")"
     ],
